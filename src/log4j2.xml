<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appenders>
        <console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} %-5level %logger{36} [%t]: %msg%n" />
        </console>
    </appenders>
    <loggers>   
        <root level="DEBUG">
            <appender-ref ref="Console" level="DEBUG" />
        </root>
    </loggers>
</configuration>